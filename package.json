{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">= 18.16 <19"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite-node src/index.ts",
    "dev:watch": "vite-node --watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "npm run clean && tsc -p tsconfig.release.json && vite build",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "pre-commit": "npx lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview-coverage": "vite preview --outDir coverage",
    "type-check": "tsc -p tsconfig.release.json",
    "lint": "rome check .",
    "lint:apply": "rome check --apply .",
    "format": "rome format .",
    "format:fix": "rome format --write ."
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "~18.16.5",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "rimraf": "~5.0.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rome": "^12.1.3",
    "ts-api-utils": "~1.0.2",
    "tslib": "~2.6.2",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vite-node": "^0.34.3",
    "vite-plugin-dts": "^3.5.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  },
  "volta": {
    "node": "18.16.0"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "rome check --apply .",
      "rome format --write ."
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "elysia": "^0.6.12",
    "express": "^4.18.2"
  }
}
